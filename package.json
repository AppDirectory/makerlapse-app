{
  "name": "cra-electron",
  "description": "CRA + Electron sitting in a tree",
  "author": "Ilias Haddad",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ffmpeg/ffmpeg": "0.6.1",
    "aperture": "^5.2.0",
    "axios": "^0.19.0",
    "cross-env": "6.0.2",
    "delay": "^4.3.0",
    "desktop-screenshot": "^0.1.1",
    "easytimer": "^1.1.1",
    "electron-is-dev": "1.1.0",
    "electron-log": "^3.0.8",
    "ffmpeg-on-progress": "^1.0.0",
    "jimp": "^0.8.5",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "os": "^0.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-rnd": "^10.1.1",
    "screenshot-desktop": "^1.10.0",
    "streamifier": "^0.1.1",
    "uniqid": "^5.1.0",
    "update-electron-app": "^1.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "4.1.2",
    "electron": "6.0.11",
    "electron-builder": "^21.2.0",
    "prettier": "^1.4.4",
    "react-scripts": "3.1.2",
    "wait-on": "3.3.0"
  },
  "build": {
    "compression": "maximum",
    "publish": [
      {
        "provider": "github",
        "owner": "IliasHad",
        "repo": "makerlapse-app",
        "token": "dc40fe2c1fb9629578458f7af870a0ee2152c13e"
      }
    ],
    "appId": "com.github.ilias.makerlapse-app",
    "asar": true,
    "productName": "Makerlapse",
    "copyright": "Copyright Â© 2019 ${author}",
    "artifactName": "${productName}.${ext}",
    "win": {
      "target": [
        "squirrel",
        "nsis",
        "zip"
      ],
      "icon": "src/assets/icons/icon.png"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/assets/icons/icon_512.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
